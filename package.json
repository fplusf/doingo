{
  "name": "electron-shadcn",
  "productName": "electron-shadcn Template",
  "version": "1.0.0",
  "description": "Electron Forge with shadcn-ui (Vite + Typescript)",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start && tsr watch --vite",
    "start:web": "vite --config vite.web.config.ts",
    "generate-routes": "router generate",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prett": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:web": "vite build --config vite.web.config.ts && npm run postbuild:web",
    "build:web:prod": "NODE_ENV=production vite build --config vite.web.config.ts && npm run postbuild:web",
    "postbuild:web": "cp dist/index.web.html dist/index.html && cp dist/index.html dist/404.html && cp src/assets/logo.png dist/logo.png && cp public/favicon.ico dist/favicon.ico",
    "serve:web": "npx serve -s dist -l 5000",
    "preview:web": "vite preview --config vite.web.config.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:all": "vitest && playwright test",
    "build:pages": "vite build --config vite.web.config.ts && mv dist docs"
  },
  "author": "FF - Farukh",
  "license": "MIT",
